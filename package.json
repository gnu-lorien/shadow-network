    "vue": "^2.6.12",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.4",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "@vue/test-utils": "^1.0.0-beta.16",
    "bootstrap": "^4.5.3",
